{
  "name": "LabHive",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "serve": "tsc -p tsconfig.browser.json && vue-cli-service serve",
    "build": "tsc && tsc -p tsconfig.browser.json && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "backend": "tsc && node dist-backend/backend/main.js"
  },
  "dependencies": {
    "argon2": "^0.26.2",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.11.0",
    "bottleneck": "^2.19.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "jsonschema": "^1.2.6",
    "jsonwebtoken": "^8.5.1",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.7",
    "nodemailer": "^6.4.6",
    "popper.js": "^1.16.1",
    "typescript": "^3.8.3",
    "uuid": "^7.0.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.16.0",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.6",
    "vue2-leaflet": "^2.5.2",
    "vuex": "^3.1.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "@types/cors": "^2.8.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.10",
    "@types/nodemailer": "^6.4.0",
    "@types/uuid": "^7.0.2",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-vuex": "^4.3.1",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "node-sass": "^4.13.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-svg": "~0.1.3",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-prototype-builtins": "off",
      "vue/no-parsing-error": [
        "error",
        {
          "unexpected-character-in-unquoted-attribute-value": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
